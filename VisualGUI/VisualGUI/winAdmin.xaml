<Window x:Name="winAdmin" x:Class="winAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualGUI"
        mc:Ignorable="d"
        Title="Administrador" Height="300" Width="300">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FFFAFEFF" Offset="0.152"/>
                <GradientStop Color="#FF86DCF0" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Menu x:Name="menu" HorizontalAlignment="Left" Height="22" VerticalAlignment="Top" Width="292">
            <MenuItem Header="Productos"/>
            <MenuItem Header="Vendedores"/>
            <MenuItem Header="Provincias"/>
            <MenuItem Header="Aiudaaaa"/>
        </Menu>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="178" Margin="10,81,0,0" VerticalAlignment="Top" Width="272"/>
    </Grid>
</Window>
